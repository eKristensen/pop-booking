{
  "name": "pop-booking-web",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "draft-js": "^0.11.0",
    "mobx": "^5.11.0",
    "mobx-react": "^6.1.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-env": "^1.2.3",
    "react-big-calendar": "^0.19.2",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-bootstrap-table-next": "^3.1.5",
    "react-bootstrap-time-picker": "^2.0.0",
    "react-bootstrap-typeahead": "^3.4.5",
    "react-calendar": "^2.19.0",
    "react-color": "^2.17.3",
    "react-date-picker": "^7.6.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-google-recaptcha-v3": "^1.3.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.3.1",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-sass src/stylesheets/scss/ -o src/stylesheets/css/",
    "watch-css": "npm run build-css && node-sass src/stylesheets/scss/ -o src/stylesheets/css --watch --recursive",
    "start-js": "react-app-env start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-env build",
    "test": "react-app-env test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
